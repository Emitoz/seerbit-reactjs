{
  "name": "seerbit-reactjs",
  "version": "1.0.5",
  "description": "Seerbit ReactJS Wrapper for quick integration",
  "author": "seerbit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seerbit/seerbit-reactjs.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "lib",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "npm run lint && rollup -c --environment NODE_ENV:production",
    "start": "tsc --watch --project tsconfig.json",
    "b": "tsc --project tsconfig.json",
    "lint": "eslint --fix --ext=js ./src",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@svgr/rollup": "^2.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.9.4"
  },
  "bugs": {
    "url": "https://github.com/seerbit/seerbit-reactjs/issues"
  },
  "homepage": "https://github.com/seerbit/seerbit-reactjs#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "seerbit",
    "react",
    "reactjs",
    "paymentgateway"
  ],
  "dependencies": {
    "@types/react": "^18.0.26",
    "react": "^18.2.0"
  }
}
